{
  "require": {
    "monolog/monolog": "^3.8",
    "phpmailer/phpmailer": "^6.0",
    "symfony/browser-kit": "^6.0",
    "symfony/console": "^6.0",
    "symfony/css-selector": "^6.0",
    "symfony/dom-crawler": "^6.0",
    "symfony/dotenv": "^7.2",
    "symfony/http-client": "^7.2",
    "symfony/process": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/php/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
